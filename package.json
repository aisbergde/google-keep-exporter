{
  "name": "google-keep-exporter",
  "version": "1.0.0",
  "description": "This is a tool which converts the Google Keep archive to markdown documents with YAML front matter. The Google Keep archive can be obtained from the Google Takeout page.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest src/**"
  },
  "author": "",
  "dependencies": {
    "cheerio": "0.22.0",
    "json2yaml": "^1.1.0",
    "moment": "^2.14.1",
    "to-markdown": "3.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
